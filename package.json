{
  "name": "kripticc-backend",
  "description": "",
  "version": "0.0.5",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Uditt Lamba",
    "email": "uditt@pm.me"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run jest",
    "lint": "ts-standard --fix",
    "dev": "ts-node-dev --notify src/",
    "start": "npm run compile && node lib/",
    "jest": "jest --forceExit",
    "compile": "shx rm -rf lib/ && tsc",
    "prepare": "npx husky install"
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "lib",
      "src/**/*.js",
      "db-data",
      "config",
      "public",
      "test"
    ]
  },
  "commitlint": {
    "extends": [
      "./node_modules/@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ],
      "header-max-length": [
        0,
        "always",
        120
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint",
      "pre-commit": "npm run lint"
    }
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.14",
    "@feathersjs/authentication-local": "^4.5.14",
    "@feathersjs/authentication-oauth": "^4.5.14",
    "@feathersjs/cli": "^4.8.0",
    "@feathersjs/configuration": "^4.5.12",
    "@feathersjs/errors": "^4.5.12",
    "@feathersjs/express": "^4.5.14",
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/socketio": "^4.5.14",
    "@feathersjs/transport-commons": "^4.5.14",
    "@levminer/speakeasy": "^1.4.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^6.1.3",
    "feathers-sequelize": "^6.3.3",
    "helmet": "^4.6.0",
    "ioredis": "^5.0.4",
    "is": "^3.3.0",
    "pg": "^8.7.3",
    "qrcode": "^1.5.0",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1",
    "serve-favicon": "^2.5.0",
    "shx": "^0.3.4",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "zod": "^3.16.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/is": "^0.0.21",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.31",
    "@types/qrcode": "^1.4.2",
    "@types/serve-favicon": "^2.5.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "7.32.0",
    "husky": "^8.0.1",
    "node-notifier": "^10.0.1",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "ts-node-dev": "^1.1.8",
    "ts-standard": "^11.0.0",
    "typescript": "^4.6.4"
  }
}
